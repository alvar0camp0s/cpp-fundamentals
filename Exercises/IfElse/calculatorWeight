#include <iostream>

using namespace std;

int main(){

    float weightPkg;
    cout<<"Insert the weight of your package: "<<endl;
    cin>>weightPkg;

    if(weightPkg<=5){
        cout<<"You need to pay $5 dollars"<<endl;
    } else if (weightPkg >=6 && weightPkg<=10){
        cout<<"You need to pay $10 dollars"<<endl;

    }  else if (weightPkg >=10){
        cout<<"You need to pay $15 dollars"<<endl;
    }
    return 0;
}